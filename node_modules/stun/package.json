{
  "name": "stun",
  "version": "1.3.1",
  "description": "Session Traversal Utilities for NAT (STUN) server.",
  "main": "src/index.js",
  "scripts": {
    "lint": "npx eslint .",
    "cover": "npx jest --coverage",
    "test": "npm run lint && npx jest",
    "test-ci": "npm run lint && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodertc/stun.git"
  },
  "keywords": [
    "webrtc",
    "stun",
    "rfc5389",
    "5389",
    "ice",
    "turn"
  ],
  "author": "Dmitry Tsvettsikh <me@reklatsmasters.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodertc/stun/issues"
  },
  "homepage": "https://github.com/nodertc/stun#readme",
  "devDependencies": {
    "@nodertc/eslint-config": "^0.2.1",
    "eslint": "^5.7.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "binary-data": "^0.6.0",
    "buffer-xor": "^2.0.2",
    "ip": "^1.1.5",
    "ip2buf": "^2.0.0",
    "is-stun": "^2.0.0",
    "turbo-crc32": "^1.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src",
      "<rootDir>/src/node_modules"
    ],
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "**/src/*.js",
      "**/src/node_modules/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest-cache",
    "haste": {
      "providesModuleNodeModules": [
        "attributes",
        "lib"
      ]
    }
  },
  "eslintConfig": {
    "extends": "@nodertc",
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "env": {
          "jest": true
        },
        "settings": {
          "import/resolver": {
            "node": {
              "moduleDirectory": [
                "node_modules",
                "src",
                "src/node_modules"
              ]
            }
          }
        },
        "rules": {
          "require-jsdoc": "off"
        }
      },
      {
        "files": [
          "examples/*.js"
        ],
        "rules": {
          "no-console": "off",
          "require-jsdoc": "off"
        }
      },
      {
        "files": [
          "**/attributes/stun-*.js"
        ],
        "rules": {
          "class-methods-use-this": "off"
        }
      }
    ]
  }
}
